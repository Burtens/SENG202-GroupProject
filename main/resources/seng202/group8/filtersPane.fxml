<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" prefWidth="290.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.group8.viewcontrollers.FiltersViewController">
   <children>
      <BorderPane>
         <left>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Filters" BorderPane.alignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <BorderPane.margin>
                  <Insets left="4.0" />
               </BorderPane.margin>
            </Text>
         </left>
         <right>
            <Button alignment="CENTER_RIGHT" defaultButton="true" mnemonicParsing="false" onAction="#applyFilters" text="Apply" BorderPane.alignment="CENTER">
               <font>
                  <Font size="16.0" />
               </font>
               <BorderPane.margin>
                  <Insets right="4.0" />
               </BorderPane.margin>
            </Button>
         </right>
         <VBox.margin>
            <Insets bottom="5.0" top="10.0" />
         </VBox.margin>
      </BorderPane>
      <Separator prefWidth="200.0" />
      <ScrollPane fx:id="scrollPane" prefHeight="700.0" vbarPolicy="ALWAYS">
         <content>
            <VBox fx:id="filterPane" prefWidth="270.0" />
         </content>
      </ScrollPane>
   </children>
</VBox>
