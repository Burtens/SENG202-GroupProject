<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<fx:root fx:id="titledPane" animated="false" contentDisplay="RIGHT" expanded="false" stylesheets="@stylesheets/filtersStyle.css" type="TitledPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <font>
        <Font size="14.0" />
    </font>
    <content>
        <VBox fx:id="extension" minHeight="-Infinity" minWidth="-Infinity" prefWidth="220.0">
        </VBox>
    </content>
    <graphic>
        <BorderPane fx:id="titleGraphic">
            <left>
                <Text fx:id="titleText" fill="#404040" strokeType="OUTSIDE" strokeWidth="0.0" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Text>
            </left>
            <right>
                <Button fx:id="clearButton" alignment="TOP_RIGHT" disable="true" mnemonicParsing="false" onAction="#clear" text="Clear" BorderPane.alignment="CENTER" />
            </right>
        </BorderPane>
    </graphic>
</fx:root>
